<h1>Editing language</h1>

<% form_for(@language, :url => admin_language_path ) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :language %><br />
    <%= f.text_field :language %>
  </p>
  <p>
    <%= f.label :tipe %><br />
    <%= f.select :tipe, ["normal","programming"] %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', admin_languages_path %>